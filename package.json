{
  "name": "af-win-audio",
  "version": "1.0.4",
  "description": "Библиотека для работы с громкость в Windows системах в NodeJS среде",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "test": "bun run --watch src/TEST.ts",
    "build": "rollup -c",
    "build:types": "tsc --emitDeclarationOnly",
    "build:all": "npm run build && npm run build:types",
    "process": "bun run --watch src/check_process.ts"
  },
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WebAFilippov/af-win-audio.git"
  },
  "keywords": [
    "node",
    "library",
    "c#",
    "process"
  ],
  "author": "AFilippov",
  "license": "MIT",
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": {
    "url": "https://github.com/WebAFilippov/af-win-audio/issues"
  },
  "homepage": "https://github.com/WebAFilippov/af-win-audio#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-typescript": "^12.1.1",
    "node-processlist": "^1.0.2",
    "rollup": "^4.24.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "typescript": "^5.6.3"  }
}
